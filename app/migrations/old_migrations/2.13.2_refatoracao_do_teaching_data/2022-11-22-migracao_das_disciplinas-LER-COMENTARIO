-- SÓ RODAR APÓS PREENCHER TODA A MATRIZ CURRICULAR DA INSTÂNCIA, OU DADOS SERÃO PERDIDOS!

insert into teaching_matrixes (teaching_data_fk, curricular_matrix_fk)
select t.id, cm.id from instructor_teaching_data t
join classroom c on c.id = t.classroom_id_fk
join edcenso_discipline ed on t.discipline_1_fk = ed.id
join curricular_matrix cm on cm.discipline_fk = ed.id
where cm.stage_fk = c.edcenso_stage_vs_modality_fk and cm.school_year = 2023;

