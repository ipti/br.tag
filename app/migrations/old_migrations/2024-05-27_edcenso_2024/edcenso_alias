
-- NÃO RODAR ISSO AQUI EM BOQUIM, POIS JÁ FOI FEITO ANTERIORMENTE
INSERT INTO edcenso_alias (register,corder,attr,cdesc,`default`,stable,year)
SELECT register,corder,attr,cdesc,`default`,stable,'2024' FROM edcenso_alias
WHERE year = 2023;
--

